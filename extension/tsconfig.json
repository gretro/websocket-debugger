{
  "compilerOptions": {
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "alwaysStrict": true,
    "baseUrl": "src",
    "jsx": "react",
    "lib": [
      "es5",
      "es6",
      "dom"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "noResolve": true,
    "newLine": "LF",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "skipLibCheck": true,
    "target": "es6",
    "sourceMap": true
  }
}